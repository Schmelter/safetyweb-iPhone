<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="FillParent">
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_width">fill_parent</item>
	</style>
	<style name="WrapContent">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">wrap_content</item>
	</style>
	<style name="Logo">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_marginTop">20dp</item>
		<item name="android:layout_gravity">center_horizontal</item>
	</style>
	<style name="LoginInput">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_gravity">center_horizontal</item>
	</style>
	<style name="LoginInput.Text">
		<item name="android:layout_width">250dp</item>
		<item name="android:layout_marginBottom">10dp</item>
	</style>
	<style name="LoginInput.Button">
		<item name="android:layout_marginTop">10dp</item>
		<item name="android:background">@drawable/transparent</item>
	</style>
	<style name="LoginMessage">
		<item name="android:textColor">#990000</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_gravity">center_horizontal</item>
		<item name="android:layout_marginBottom">5dp</item>
		<item name="android:textStyle">bold</item>
	</style>
	<style name="Copyright">
		<item name="android:textColor">#ababab</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_gravity">center_horizontal|bottom</item>
		<item name="android:layout_marginBottom">15dp</item>
		<item name="android:textStyle">bold</item>
	</style>
	<style name="Header">
		<item name="android:layout_height">45px</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:background">@drawable/header_background</item>
	</style>
	<style name="HeaderLogo">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">30px</item>
		<item name="android:layout_gravity">center</item>
	</style>
	<style name="ChildRow">
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:padding">10dp</item>
	</style>
	<style name="ChildRowRight">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_marginLeft">5dp</item>
	</style>
	<style name="ChildRowImage">
		<item name="android:layout_height">60dp</item>
		<item name="android:layout_width">60dp</item>
		<item name="android:layout_marginRight">10dp</item>
	</style>
	<style name="ChildRowInfoContainer">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">fill_parent</item>
	</style>
	<style name="ChildRowInfo">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_gravity">center_horizontal</item>
		<item name="android:layout_marginRight">30dp</item>
	</style>
	<style name="ChildRowText">
		<item name="android:singleLine">true</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">wrap_content</item>
	</style>
	<style name="ChildRowText.Name">
		<item name="android:textColor">#0099cc</item>
		<item name="android:textSize">20dp</item>
		<item name="android:textStyle">bold</item>
	</style>
	<style name="ChildRowText.More">
		<item name="android:textColor">#0099cc</item>
		<item name="android:textSize">16dp</item>
		<item name="android:textStyle">bold</item>
	</style>
	<style name="ChildRowTextMoreContainer">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_gravity">bottom</item>
	</style>
	<style name="ChildRowTextMoreArrow">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginTop">5dp</item>
		<item name="android:layout_marginLeft">3dp</item>
	</style>
	<style name="ChildRowText.Small">
		<item name="android:textSize">11dp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_marginLeft">10dp</item>
	</style>
	<style name="ChildRowText.Small.Numbers">
		<item name="android:layout_gravity">right</item>
	</style>
	<style name="ChildRowTextNumbersContainer">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">50dp</item>
	</style>
	<style name="Notification">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_marginTop">1dp</item>
		<item name="android:layout_marginBottom">2dp</item>
	</style>
	<style name="ChildDetailList">
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:listSelector">@android:color/transparent</item>
	</style>
	<style name="ChildDetailRow">
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:padding">5dp</item>
	</style>
	<style name="ChildDetailRowRight">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">fill_parent</item>
	</style>
	<style name="ChildDetailRowActivity">
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_width">25px</item>
	</style>
	<style name="ChildDetailRowActivityImage">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_gravity">center_horizontal</item>
		<item name="android:layout_marginTop">10dp</item>
	</style>
	<style name="ChildDetailRowServiceImage">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">16px</item>
		<item name="android:layout_marginRight">5dp</item>
		<item name="android:layout_marginTop">1dp</item>
	</style>
	<style name="ChildDetailRowLoading">
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_gravity">center_horizontal</item>
		<item name="android:layout_marginTop">5dp</item>
		<item name="android:visibility">gone</item>
	</style>
	<style name="ChildDetailRowLoadingProgress">
		<item name="android:layout_width">20dp</item>
		<item name="android:layout_height">20dp</item>
		<item name="android:layout_marginTop">3dp</item>
		<item name="android:layout_marginRight">10dp</item>
	</style>
	<style name="ChildDetailRowLoadingMessage">
		<item name="android:layout_height">40dp</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:textSize">18dp</item>
		<item name="android:textStyle">bold</item>
	</style>
</resources>
